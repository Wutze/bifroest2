          "query": "import \"timezone\"\ntimezone.location(name: \"Europe/Berlin\")\ntimezone.fixed(offset: 0h)\nfrom(bucket: \"iptables\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"firewall_log\")\n  |> filter(fn: (r) => contains(value: r[\"_field\"], set: [\"src\", \"dst\", \"dst_port\", \"action\"]))\n  |> limit(n: 50)\n  |> pivot(rowKey: [\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _value: \"\" + string(v: r.src) + \" > \" + string(v: r.dst) + \" | \" + string(v: r.dst_port) + \":\" + string(v: r.proto) + \" | \" + string(v: r.action),\n      dst: r.dst,\n      src: r.src,\n      dst_port: r.dst_port,\n      zeit: string(v: r._time)\n  }))\n  |> sort(columns: [\"_time\"], desc: true)\n",
          "refId": "A"
        }
      ],
